<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from TempService WSDL service -->

  <service name="Temp" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://DefaultNamespace"/>
      <parameter name="wsdlServiceElement" value="TempService"/>
      <parameter name="schemaQualified" value="http://DefaultNamespace"/>
      <parameter name="wsdlServicePort" value="Temp"/>
      <parameter name="className" value="DefaultNamespace.TempSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Temp"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="convertFahrenheitToCelsius" qname="operNS:convertFahrenheitToCelsius" xmlns:operNS="http://DefaultNamespace" returnQName="retNS:convertFahrenheitToCelsiusReturn" xmlns:retNS="http://DefaultNamespace" returnType="rtns:double" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:temperature" xmlns:pns="http://DefaultNamespace" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="convertFahrenheitToCelsius"/>

  </service>
</deployment>
